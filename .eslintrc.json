{
  "env": {
    "es2023": true,
    "node": true
  },
  "extends": [
    "standard",
    "plugin:@eslint-community/eslint-comments/recommended",
    "plugin:import/recommended",
    "plugin:n/recommended",
    "plugin:promise/recommended",
    "plugin:security/recommended-legacy" // TODO: (jmorris2) convert post elsint 9.0.0
  ],
  "globals": {},
  "parserOptions": {
    "ecmaVersion": 2023,
    "sourceType": "module"
  },
  "plugins": [
    "@eslint-community/eslint-comments",
    "import",
    "n",
    "promise",
    "security"
  ]
}
